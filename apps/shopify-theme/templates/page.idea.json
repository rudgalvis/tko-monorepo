    "1647179955e138dcaa": {
      "type": "static-onlytext-alt",
      "disabled": true,
      "settings": {
        "onlyTextlayout_": "top-bottom",
        "headingOnlyTextCTA_": "it all started with an ad  printed in our local paper",
        "maxwidthoftitle_": 373,
        "maxwidthoftitlemobiled_": 200,
        "bodytextOnlyTextCTA_": "We're not joking when we say we do some things traditionally around here. \nWe started The Knotty Ones with the dream of providing stable jobs, fair wages, and financial independence to women in marginalized areas of Lithuania, where these opportunities didn’t previously exist. We wanted to create something that made you feel good wearing it. Not only because you looked great, but also because you knew the woman who made your sweater got paid fairly and worked in a safe environment. We work with both artisans and local vetted factories to create opportunities in these rural communities. We honor our artisans, we learn from them, and we grow together.",
        "maxwidthoftext_": 1000,
        "maxwidthoftextmobile_": 280,
        "showurlOnlyTextCTA_": false,
        "linkOnlyTextCTA_": "",
        "linktextOnlyTextCTA_": "",
