    "1644594325eebf8fda": {
      "type": "static-two-column-alt3",
      "settings": {
        "borderlayouttwoclthreealt": "only-bottom",
        "altsectionlayouttwthree": "image-text",
        "nopadding": false,
        "altthreeimage": "shopify://shop_images/1-_MTK-Section-Remote-Working_1080x1080_667cbc2f-c46b-4a6f-9983-d87a5379acd0.webp",
        "altheadingclthree": "Remote working (and flexible hours) before it was cool",
        "h2maxwidthdesktop": 446,
        "h2maxwidthmobile": 300,
        "altbodyclthree": "<p>Our artisans who work on handmade pieces choose schedules that work for them and their families, whether that's part time, full time or while their kids are napping. They can work from the comfort of their homes, with everything they need to work safely and comfortably.</p>",
        "pmaxwidthdesktop": 900,
        "pmaxwidthmobile": 300,
        "padding_sizeup": 20,
        "padding_sizedown": 12,
        "padding_sizeupm": 15,
        "padding_sizedownm": 15,
