    "164052340611e6cfc0": {
      "type": "static-two-column",
      "settings": {
        "borderlayouttwocl": "only-bottom",
        "textposition": "space-between",
        "sectionlayouttwocl": "image-text",
        "headingcl": "Deadstock? We don’t know her",
        "showtitletwo": true,
        "h2maxwidthdesktop": 450,
        "h2maxwidthmobile": 280,
        "bodycl": "We have our handmade production and then we have our “fully fashioned” production. That’s an industry term that basically means each sweater piece is individually factory-knitted and then sewn together by hand. This zero-waste approach is pretty cool because it ensures no leftover scraps - we literally knit the shapes that’ll be sewn together.",
        "pmaxwidthdesktop": 900,
        "pmaxwidthmobile": 252,
        "cl-image": "shopify://shop_images/STB-Section-3-Deadstock-we-dont-know-her_938x_d11207a3-3d33-4fea-ab6a-9ae3a86791f6.webp",
        "padding_sizeupm_section": 33,
        "padding_sizedownm_section": 28,
        "padding_sizeup": 19,
        "padding_sizedown": 9,
        "padding_sizeupm": 10,
        "padding_sizedownm": 15,
