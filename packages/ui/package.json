{
  "name": "ui",
  "version": "0.0.1",
  "scripts": {
    "dev:storybook": "storybook dev -p 6006 --ci",
    "dev:regular": "vite dev --config vite.config.js",
    "dev:for-theme": "vite build --config vite.config.build.js --mode production --watch",
    "build": "vite build && npm run prepack",
    "build-for-theme": "vite build --config vite.config.build.js --mode production",
    "build-storybook": "storybook build",
    "rollup": "npx rollup -c",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@storybook/addon-actions": "^8.6.12",
    "@storybook/addon-essentials": "^8.5.2",
    "@storybook/addon-interactions": "^8.5.2",
    "@storybook/addon-svelte-csf": "^5.0.0-next.23",
    "@storybook/blocks": "^8.5.2",
    "@storybook/svelte": "^8.5.2",
    "@storybook/sveltekit": "^8.5.2",
    "@storybook/test": "^8.5.2",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/eslint": "^9.6.0",
    "@types/node": "^22.10.1",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.11.2",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.6",
    "publint": "^0.2.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-svelte": "^7.2.2",
    "storybook": "^8.5.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-preprocess": "^6.0.3",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.11",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-env-compatible": "^2.0.1",
    "vite-plugin-singlefile": "^2.1.0",
    "vite-plugin-static-copy": "^2.0.0",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@shopify/storefront-api-client": "^1.0.7",
    "@supabase/supabase-js": "^2.48.1",
    "@tailwindcss/vite": "^4.1.4",
    "common-utils": "workspace:*",
    "dotenv": "^16.4.7",
    "sass": "^1.82.0",
    "storefront-api": "workspace:*",
    "swiper": "^11.1.14",
    "tailwindcss": "^4.1.4"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
