.btn-darrow:after{
    content: "→";
    position: relative;
    font-family: 'Formular';
    font-weight: 500;
    left:8px;
    transition: 0.3s ease-in-out;
    -webkit-transition: 0.3s ease-in-out;
    top: -.5px;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale; 
 }



 .btn-larrow:after{
    content: "↓";
     position: relative;
     /*font-family: 'Formular';*/
     top:0px;
     left:4px;
     transition: 0.3s ease-in-out;
     -webkit-transition: 0.3s ease-in-out;
     text-transform: uppercase;
    -webkit-font-smoothing: antialiased;

 }

 .btn-monument:after{
    content: "→";
    position: relative;
    font-family: 'Monument';
    font-weight: 500;
    left:8px;
    transition: 0.3s ease-in-out;
    -webkit-transition: 0.3s ease-in-out;
    top: -.5px;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale; 

 }

 .btn-click:after{
    content: "↑";
 }


 .btn-space:after{
     margin-left:3px;
     top:1px;
    
 }



 .btn-larrow-buy{
     position: relative;
     font-family: 'Monument Regular';
     top:-1px;
     left:8px;
     text-transform: uppercase;
